(def factorial (fn x
  (if (= x 1)
    1
    (* x (factorial (- x 1))))))

(print (factorial 5))

(def fib (fn n
  (if (= n 0) 0
    (if (= n 1) 1
      (+ (fib (- n 1)) (fib (- n 2)))))))

(print (fib 5))

(def ackermann (fn n
  (if (= (fst n) 0)
    (+ (snd n) 1)
    (if (= (snd n) 0)
      (ackermann (- (fst n) 1) 1)
      (ackermann (- (fst n) 1) (ackermann (fst n) (- (snd n) 1)))))))

(print (ackermann 3 3))

